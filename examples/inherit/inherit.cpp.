char * Father::fatherMethod (){
JNIEnv * curEnv = getCurrentEnv();
if (jstringfatherMethodID==NULL) { /* Use the cache Luke */
	jstringfatherMethodID = curEnv->GetMethodID(getInstanceClass(), "fatherMethod", "()Ljava/lang/String;" ) ;
if (jstringfatherMethodID == NULL) {
std::cerr << "Could not access to the method " << "fatherMethod" << std::endl;
curEnv->ExceptionDescribe();
exit(EXIT_FAILURE);
}
}
 jstring res =  static_cast<jstring>( curEnv->CallObjectMethod( getInstance(), jstringfatherMethodID ));
if (curEnv->ExceptionCheck()) {
curEnv->ExceptionDescribe() ;
}


const char *tempString = curEnv->GetStringUTFChars(res, 0);
char * myStringBuffer = new char[strlen(tempString) + 1];
strcpy(myStringBuffer, tempString);
curEnv->ReleaseStringUTFChars(res, tempString);
curEnv->DeleteLocalRef(res);
if (curEnv->ExceptionCheck()) {
curEnv->ExceptionDescribe() ;
}

return myStringBuffer;

}


